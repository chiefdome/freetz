+<script type="text/javascript">
+<? include ../html/$var:lang/js/jsl.js ?>
+var g_mldUnsubscribe	= "Wollen Sie das Mobilteil abmelden?";
+var g_mldDectActivate	= "Eine Veränderung dieser Einstellung macht einen Neustart des Geräts erforderlich!";
+var g_mldReboot     	= "Das Gerät wird jetzt neu gestartet!";
+var g_New = -1;
+function uiDoOnLoad() {
+	var bFound = false;
+
+	for (var i=0; i<=5; i++) {
+		if (jslGetValue("uiPostSubscribed"+i) == "1") {
+			jslSetChecked("uiViewSubscribed"+i, jslGetValue("uiPostSubscribed"+i) == "1");
+			jslSetChecked("uiViewType"+i, jslGetValue("uiPostType"+i) == "1");
+			jslDisplay("uiViewHandset"+i, true); bFound=true;
+		} else {
+			jslDisable("uiViewDelete"+i)
+			if (g_New == -1) g_New = i;
+		}
+	}
+	jslDisplay("uiNichts", !bFound);
+	jslSetEnabled("uiViewPage", bFound);
+
+//	jslSetChecked("uiViewDectActivated", jslGetValue("uiPostDectActivated") == "1");
+	if (jslGetValue("uiPostDectActivated") == "1") {
+		jslSetChecked("uiViewDectActivated", true);
+		uiDectElementsEnable(1);
+	} else {
+		jslSetChecked("uiViewDectActivated", false);
+		uiDectElementsEnable(0);
+	}
+
+	if (g_New == -1) jslDisable("uiViewNew");
+	jslZebrastreifen("tHandsets");
+}
+function uiOnChangeActivated(idui, idpost) {
+	if (jslGetChecked(idui)) {
+		jslSetValue(idpost, "1");
+		return 1;
+	} else {
+		jslSetValue(idpost, "0");
+		return 0;
+	}
+}
+function uiOnChangeDectActivated() {
+	if (!confirm(g_mldDectActivate)) return;
+	uiDectElementsEnable(uiOnChangeActivated("uiViewDectActivated", "uiViewDectActivated"));
+}
+function uiDectElementsEnable(iActive) {
+	jslSetEnabled("uiViewNew", iActive == 1);
+	jslSetEnabled("uiViewPage", iActive == 1);
+	jslSetEnabled("uiViewDateTime", iActive == 1);
+
+	for (var i=0; i<=5; i++) {
+		jslSetEnabled("uiViewDelete"+i, iActive == 1);
+	}
+}
+function uiDoSave() {
+	if (jslGetValue("uiPostDectActivated") != (jslGetChecked("uiViewDectActivated") ? "1":"0")) { 
+		jslSetValue("uiPostDectActivated", jslGetChecked("uiViewDectActivated") ? "1":"0");
+		jslEnable("uiPostDectActivated");
+
+		document.getElementById("uiPostVarName").name = "logic:command/reboot";
+		jslSetValue("uiPostVarName", "../gateway/commands/saveconfig.html");
+		jslSetValue("uiPostGetPage", "../html/reboot.html");
+		document.getElementById("uiPostForm").submit();
+
+		}
+// in foneditdect.frm: <input type="hidden" name="logic:command/reboot" id="uiPostReboot">
+//		jslSetValue("uiPostReboot", "1");
+//		jslEnable("uiPostReboot");
+//	}
+//<? if eq '$var:pagename' 'foneditdect' `
+//	jslFormSubmitEx("fon", "foneditdect");
+//` `	
+//	jslFormSubmitEx("fon", "foneditisdn");
+//` ?>
+}
+function uiDoHelp() {
+	jslPopHelp("hilfe_fon_editdect");
+}
+function uiDoCancel() {
+	jslGoTo("", "");
+}
+function uiDoRefresh() {
+<? if eq '$var:pagename' 'foneditdect' `
+	jslGoTo("fon", "foneditdect");
+` `	
+	jslGoTo("fon", "foneditisdn");
+` ?>
+}
+function uiDoIsdn() {
+	jslGoTo("fon", "isdn");
+}
+function uiDoListIsdn() {
+	jslGoTo("fon", "fonlistisdn");
+}
+function uiDoMsn() {
+	jslSetValue("uiPostPageMaster", "isdn");
+	jslGoTo("fon", "isdnmsn");
+}
+function uiDoOptionen() {
+	jslGoTo("fon", "isdnoptionen");
+}
+function uiDoSetupIsdn() {
+	jslGoTo("fon", "fonsetupisdn");
+}
+function uiDoDect() {
+	jslGoTo("fon", "dect");
+}
+function uiDoListDect() {
+	jslGoTo("fon", "fonlistdect");
+}
+function uiDoDectMsn() {
+	jslGoTo("fon", "dectmsn");
+}
+function uiDoSetupDect() {
+	jslGoTo("fon", "fonsetupdect");
+}
+function uiDoDelete(nr) {
+	if (!confirm(g_mldUnsubscribe)) return;
+	jslSetValue("uiPostUnsubscribe", nr);
+	jslEnable("uiPostUnsubscribe");
+	jslSubmitFormEx("fon", "", "");
+}
+function uiDoNew() {
+	jslSetValue("uiPostStartSubscription", "0");
+	jslEnable("uiPostStartSubscription");
+	jslSubmitFormEx("fon", "", "");
+}
+function uiDoPage () {
+	jslSetValue("uiPostPageHandsets", "0");
+	jslEnable("uiPostPageHandsets");
+	jslSubmitFormEx("fon", "", "");
+}
+function uiDoDateTime () {
+	jslSetValue("uiPostDateTime", "0");
+	jslEnable("uiPostDateTime");
+	jslSubmitFormEx("fon", "", "");
+}
+</script>
