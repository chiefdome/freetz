config EXTERNAL_FREETZ_PACKAGE_FFMPEG_ffmpeg
	depends on EXTERNAL_ENABLED && FREETZ_PACKAGE_FFMPEG_ffmpeg
	boolean "ffmpeg"
	default n
	help
