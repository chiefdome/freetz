--- Makefile.config
+++ Makefile.config
@@ -0,0 +1,111 @@
+DEFAULT_TARGET=nonmerge
+NETPBMLIBTYPE=unixshared
+STATICLIB_TOO=y
+
+BUILD_FIASCO=N
+
+FAKEROOTDIR=
+TARGET_CROSS_PREFIX=
+
+TOOLCHAIN_PREFIX=
+ifneq ($(strip $(FAKEROOTDIR)),)
+ifneq ($(strip $(TARGET_CROSS_PREFIX)),)
+TOOLCHAIN_PREFIX=$(FAKEROOTDIR)/usr/bin/$(TARGET_CROSS_PREFIX)
+endif
+endif
+
+CC=$(TOOLCHAIN_PREFIX)gcc
+LD=$(CC)
+AR=$(TOOLCHAIN_PREFIX)ar
+RANLIB=$(TOOLCHAIN_PREFIX)ranlib
+
+LINKERISCOMPILER=Y
+LINKER_CAN_DO_EXPLICIT_LIBRARY=Y
+
+INTTYPES_H=<inttypes.h>
+HAVE_INT64=Y
+
+#CFLAGS=-O3 -ffast-math  -pedantic -fno-common -Wall -Wno-uninitialized -Wmissing-declarations -Wimplicit -Wwrite-strings -Wmissing-prototypes -Wundef
+CFLAGS=
+CFLAGS_MERGE=$(CFLAGS)
+
+CC_FOR_BUILD=$(CC)
+LD_FOR_BUILD=$(LD)
+CFLAGS_FOR_BUILD=$(CFLAGS)
+
+CFLAGS_SHLIB=
+SHLIB_CLIB=
+
+NETPBMLIBSUFFIX=so
+STATICLIBSUFFIX=a
+EXE=
+
+DLLVER=
+
+STRIPFLAG=-s
+SYMLINK=ln -s
+MANPAGE_FORMAT=nroff
+LEX=
+
+LDSHLIB=-shared -Wl,-soname,$(SONAME)
+LDRELOC=NONE
+
+NEED_RUNTIME_PATH=N
+RPATHOPTNAME=-rpath
+NETPBMLIB_RUNTIME_PATH=
+
+TIFFLIB=NONE
+TIFFHDR_DIR=
+TIFFLIB_NEEDS_JPEG=Y
+TIFFLIB_NEEDS_Z=Y
+
+JPEGLIB=$(FAKEROOTDIR)/usr/lib/libjpeg.so.62
+JPEGHDR_DIR=$(FAKEROOTDIR)/usr/include
+
+PNGVER=12
+PNGLIB=$(FAKEROOTDIR)/usr/lib/libpng$(PNGVER).so.0
+PNGHDR_DIR=$(FAKEROOTDIR)/usr/include/libpng$(PNGVER)
+
+ZLIB=$(FAKEROOTDIR)/usr/lib/libz.so.1
+ZHDR_DIR=$(FAKEROOTDIR)/usr/include
+
+JBIGLIB=$(BUILDDIR)/converter/other/jbig/libjbig.a
+JBIGHDR_DIR=$(SRCDIR)/converter/other/jbig
+
+JASPERLIB=$(INTERNAL_JASPERLIB)
+JASPERHDR_DIR=$(INTERNAL_JASPERHDR_DIR)
+JASPERDEPLIBS=
+
+URTLIB=$(BUILDDIR)/urt/librle.a
+URTHDR_DIR=$(SRCDIR)/urt
+
+X11LIB=NONE
+X11HDR_DIR=
+
+LINUXSVGALIB=NONE
+LINUXSVGAHDR_DIR=
+
+OMIT_NETWORK=Y
+NETWORKLD=
+
+VMS=
+
+DONT_HAVE_PROCESS_MGMT=N
+
+INSTALL=$(SRCDIR)/buildtools/install.sh
+PKGDIR_DEFAULT=/tmp/netpbm
+SHLIBPREFIXLIST=lib
+NETPBMSHLIBPREFIX=$(firstword $(SHLIBPREFIXLIST))
+
+PKGMANDIR=man
+SUFFIXMANUALS1=1
+SUFFIXMANUALS3=3
+SUFFIXMANUALS5=5
+NETPBM_DOCURL=http://netpbm.sourceforge.net/doc/
+
+INSTALL_PERM_BIN=755
+INSTALL_PERM_LIBD=755
+INSTALL_PERM_LIBS=644
+INSTALL_PERM_HDR=644
+INSTALL_PERM_MAN=644
+INSTALL_PERM_DATA=644
