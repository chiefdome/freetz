#!/bin/sh

cat << EOF
STARTDELAY=$CHECKMAILD_STARTDELAY
INTERVALL=$CHECKMAILD_INTERVALL

LOGGING=N
LOGMODE=S
LEDNOTIFY=$CHECKMAILD_LEDNOTIFY
LEDMAJOR=$CHECKMAILD_LEDMAJOR
LEDMINOR=$CHECKMAILD_LEDMINOR
TELNOTIFY=$CHECKMAILD_TELNOT
NUMBER=$CHECKMAILD_PHONENUMBER
SIP=$CHECKMAILD_SIPACCOUNT
FON=$CHECKMAILD_SUBSTATION

NAME0=$CHECKMAILD_USER0
POP30=$CHECKMAILD_POPSERVER0
IMAP0=$CHECKMAILD_IMAPSERVER0
USER0=$CHECKMAILD_USERNAME0
PASS0=$CHECKMAILD_PASSWORD0
INBOX0=$CHECKMAILD_INBOX0

NAME1=$CHECKMAILD_USER1
POP31=$CHECKMAILD_POPSERVER1
IMAP1=$CHECKMAILD_IMAPSERVER1
USER1=$CHECKMAILD_USERNAME1
PASS1=$CHECKMAILD_PASSWORD1
INBOX1=$CHECKMAILD_INBOX1

NAME2=$CHECKMAILD_USER2
POP32=$CHECKMAILD_POPSERVER2
IMAP2=$CHECKMAILD_IMAPSERVER2
USER2=$CHECKMAILD_USERNAME2
PASS2=$CHECKMAILD_PASSWORD2
INBOX2=$CHECKMAILD_INBOX2

CFGNOTIFY=$CHECKMAILD_CFGNOTIFY
RECVMSG=$CHECKMAILD_RECVMSG

EOF


