config EXTERNAL_FREETZ_PACKAGE_OPENSSH
	depends on EXTERNAL_ENABLED && FREETZ_PACKAGE_OPENSSH && FREETZ_SHOW_ADVANCED
	bool "OpenSSH"
	default n
	help
		externals these file(s):
			/usr/lib/sftp-server
			/usr/sbin/sshd
			/usr/bin/ssh-agent
			/usr/bin/ssh-add
			/usr/bin/ssh
			/usr/bin/scp
			/usr/bin/ssh-keysign
			/usr/bin/ssh-rand-helper
			/usr/bin/sftp
			/usr/bin/ssh-keygen
			/usr/bin/ssh-keyscan

