config DS_PACKAGE_PPPD
	bool "pppd 2.4.3"
	depends on DS_REPLACE_KERNEL && (DS_PACKAGE_PPTP || DS_PACKAGE_PPTPD)
	select DS_LIB_libcrypt
	select DS_LIB_libdl
	select DS_MODULE_ppp_async
	select DS_MODULE_ppp_deflate
	select DS_MODULE_ppp_generic
	select DS_MODULE_ppp_mppe_mppc
	select DS_MODULE_arc4
	select DS_MODULE_sha1
	default n
	help
		An implementation of the Point-to-point protocol.

		http://www.samba.org/ppp/
