--- linux-2.6.28/drivers/net/avm_cpmac/Makefile	2012-03-13 13:35:56.000000000 +0100
+++ linux-2.6.28/drivers/net/avm_cpmac/Makefile	1970-01-01 01:00:00.000000000 +0100
@@ -1,49 +0,0 @@
-#
-# Makefile for the "cpmac" family network device drivers.
-# Copyright (C) 2006,2007,2008,2009,2010 AVM GmbH <fritzbox_info@avm.de>
-#
-.PHONY: DO_COPY FORCE
-
-EXTRA_CFLAGS := -DCPMAC_SIGNAL_CONGESTION -DCPPHY_USE_SWITCH
-#EXTRA_CFLAGS += -Wundef
-#EXTRA_CFLAGS += -Wshadow
-#EXTRA_CFLAGS += -Wmissing-declarations
-#EXTRA_CFLAGS += -Wmissing-field-initializers
-#EXTRA_CFLAGS += -Wpadded
-#EXTRA_CFLAGS += -Wunreachable-code
-EXTRA_CFLAGS += -Wall -Wextra -Werror
-EXTRA_CFLAGS += -DAVM_CPMAC_VERSION="\"`svn info $(src) | grep URL | sed -e "s/^.*\/\([0-9.]\+\)-[^\/]*/\1/g"`  -  Revision `svnversion -cn $(src)`  -  `date +\"%a  %e.%m.%Y  %H:%M:%S\"` \""
-
-# declare multi-module main driver.
-obj-$(CONFIG_AVM_CPMAC)	+= avm_cpmac.o
-
-# cpphy modules linked to cpmac part because of difficulties using separate lib.a for cpphy */
-avm_cpmac-y := cpmac_main.o cpmac_if.o cpphy_cppi.o cpphy_if.o cpphy_if_g.o cpphy_mdio.o \
-			   cpphy_main.o cpphy_misc.o cpphy_adm6996.o cpphy_ar8216.o cpmac_eth.o \
-			   cpphy_entry.o cpphy_ar8216_ar.o cpphy_mgmt.o \
-			   cpmac_fusiv_if.o cpmac_puma_if.o cpphy_switch.o cpmac_product_conf.o
-			
-$(obj)/cpphy_entry.o: $(src)/cpphy_entry.c DO_COPY FORCE
-
-DO_COPY:
-	mkdir -p $(FRITZ_BOX_BUILD_DIR)/$(KERNEL_BUILD)/filesystem/include/linux 
-	cp -f $(src)/linux_avm_cpmac.h $(FRITZ_BOX_BUILD_DIR)/$(KERNEL_BUILD)/filesystem/include/linux/avm_cpmac.h
-
-extra-y	+= cpmacconfig
-
-cpmacconfig-objs	:= cpmacconfig.o
-
-CFLAGS_cpmacconfig.o  += $(shell xx=$(CROSS_COMPILE); echo "-isystem$${xx%/*}/../include")
-
-$(src)/cpmacconfig: $(src)/cpmacconfig.o FORCE
-	$(CROSS_COMPILE)gcc $(CFLAGS) $(CFLAGS_cpmacconfig.o) -o $(src)/cpmacconfig $(src)/cpmacconfig.o
-	$(CROSS_COMPILE)strip -s $(src)/cpmacconfig 
-	if [ ! -d $(INSTALL_MOD_PATH)/sbin ]; then \
-		echo "  MKDIR $(INSTALL_MOD_PATH)/sbin" ; \
-		mkdir -p "$(INSTALL_MOD_PATH)/sbin" ; \
-	fi
-	cp $(src)/cpmacconfig $(INSTALL_MOD_PATH)/sbin
-
-$(src)/cpmacconfig.o: $(src)/cpmacconfig.c
-	$(CROSS_COMPILE)gcc $(CFLAGS) $(CFLAGS_cpmacconfig.o) -c -o $(src)/cpmacconfig.o $(src)/cpmacconfig.c
-
